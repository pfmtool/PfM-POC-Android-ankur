<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
    <filter>
        <filter-name>ObjectifyFilter</filter-name>
        <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>ObjectifyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>queryUser</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.QueryUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>queryUser</servlet-name>
        <url-pattern>/queryUser.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>queryPortfolio</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.QueryPortfolioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>queryPortfolio</servlet-name>
        <url-pattern>/queryPortfolio.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addUser</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.AddUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addUser</servlet-name>
        <url-pattern>/addUser.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>userAuthentication</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.UserAuthenticationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userAuthentication</servlet-name>
        <url-pattern>/userAuthentication.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>addPortfolio</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.AddPortfolioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addPortfolio</servlet-name>
        <url-pattern>/addPortfolio.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>deleteUser</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.DeleteUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deleteUser</servlet-name>
        <url-pattern>/deleteUser.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>deletePortfolio</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.DeletePortfolioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deletePortfolio</servlet-name>
        <url-pattern>/deletePortfolio.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>updateUser</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.UpdateUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateUser</servlet-name>
        <url-pattern>/updateUser.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>updatePortfolio</servlet-name>
        <servlet-class>com.PfM_Demo_Backend.UpdatePortfolioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updatePortfolio</servlet-name>
        <url-pattern>/updatePortfolio.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SystemServiceServlet</servlet-name>
        <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
        <init-param>
            <param-name>services</param-name>
            <param-value>com.PfM_Demo_Backend.RegistrationEndpoint,
                com.PfM_Demo_Backend.MessagingEndpoint
            </param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>SystemServiceServlet</servlet-name>
        <url-pattern>/_ah/spi/*</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>queryUser.do</welcome-file>
    </welcome-file-list>
</web-app>